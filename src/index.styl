#recipe-editor label
  display: block

#app > div
  display: flex

#recipe-index
  list-style-type: none
  padding: 0
  margin: 0

  li
    padding: 10px 0
    margin: 0 5px
    height: 40px
    line-height: @height
    border-bottom: 1px #ccc solid

    a
      display: inline-block
      min-width: 200px
